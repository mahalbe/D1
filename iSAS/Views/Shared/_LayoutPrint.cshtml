<body class="">
    <div id="spinner" class="spinnerMain" style="display:none;">
        <div class="spinner">
            <img id="img-spinner" src="../Images/System/Loading.gif" alt="Loading" style="width:100%;height:100%;" />
        </div>
    </div>
    <div class="">
        @RenderBody()
    </div>
</body>

@model  ISas.Entities.CommonEntities.ReportHeaderEntities

<div id="reportHeaderDiv_WithoutLogo">
    <table id="tblReportHeader_WithoutLogo" cellpadding="0" cellspacing="0" style="width:100%;">
        <tr>
            @*<td>
                <i><img src="@ViewBag.ImageData" height="140" width="150" /> </i>
            </td>*@
            <td style="text-align:center;">
                <span style="font-size:22px;font-family:'Times New Roman';">
                    <strong>@Html.DisplayFor(r => Model.Header1), @Html.DisplayFor(r => Model.Header2)</strong>
                </span><br />
                <span style="font-size:18px;font-family:'Times New Roman';">
                    <strong>@Html.DisplayFor(r => Model.Header3), @Html.DisplayFor(r => Model.Header4)</strong>
                </span><br />
                <span style="font-size:18px;font-family:'Times New Roman';">
                    <strong>@Html.DisplayFor(r => Model.ReportName)</strong>
                </span>
            </td>
        </tr>
    </table>
</div>
